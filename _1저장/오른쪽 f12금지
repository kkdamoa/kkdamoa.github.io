        // 키보드 이벤트로 특정 단축키를 차단합니다 (예: F12, Ctrl+U)
        document.addEventListener('keydown', function (e) {
            if (e.key === "F12") {
                e.preventDefault();
                alert("F12 개발자 도구는 사용하실 수 없습니다.");
            }
            if ((e.ctrlKey && (e.key === "u" || e.key === "U")) || 
                (e.ctrlKey && e.shiftKey && e.key === "I")) {
                e.preventDefault();
                alert("소스 코드는 보호되고 있습니다.");
            }
        });

        // 우클릭 차단: 마우스 오른쪽 버튼 클릭을 비활성화합니다.
        document.addEventListener('contextmenu', function (e) {
            e.preventDefault();
            alert("우클릭은 비활성화되었습니다.");
        });

        // 개발자 도구 감지: 개발자 도구 열림 여부를 감지합니다.
        (function devToolsChecker() {
            const element = new Image();
            Object.defineProperty(element, 'id', {
                get: function () {
                    alert('개발자 도구가 감지되었습니다. 사용을 멈추세요!');
                    throw new Error('개발자 도구 사용 금지!');
                }
            });
            setInterval(function () {
                console.log(element);
            }, 1000);
        })();
    </script>
